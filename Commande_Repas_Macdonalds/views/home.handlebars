<script>
    var Tout = document.getElementById("Tout");
    var Nos_Menus = document.getElementById("Nos_Menus");
    var Nos_Sandwichs = document.getElementById("Nos_Sandwichs");
    var Petites_faims = document.getElementById("Petites_faims");
    var Nos_salades = document.getElementById("Nos_salades");
    var Nos_desserts = document.getElementById("Nos_desserts");
    var Nos_boissons = document.getElementById("Nos_boissons");
    var Nos_frites = document.getElementById("Nos_frites");
    var Nos_sauces = document.getElementById("Nos_sauces");
    var McCafe = document.getElementById("McCafe");

    var Langue_FR =document.getElementsByClassName("Langue_FR");
    var Langue_AN =document.getElementsByClassName("Langue_AN");
    var Langue_AR =document.getElementsByClassName("Langue_AR");

    var menu = document.getElementsByClassName("boxproduct_menu");
    var boissons = document.getElementsByClassName("boxproduct_boissons");
    var sandwich = document.getElementsByClassName("boxproduct_sandwich");
    var petitesFaim = document.getElementsByClassName("boxproduct_petitesFaim");
    var salade = document.getElementsByClassName("boxproduct_salade");
    var dessert = document.getElementsByClassName("boxproduct_dessert");
    var frite = document.getElementsByClassName("boxproduct_frite");
    var sauces = document.getElementsByClassName("boxproduct_sauces");
    var cafe = document.getElementsByClassName("boxproduct_cafe");
    var i;

    function onload_start(){

        alert(sessionStorage.getItem("modeCommande"));

        if (typeof(Storage) !== "undefined") {
            // Retrieve
            //FRANCAIS
            if(sessionStorage.getItem("langue") == "FR"){

                for (i = 0; i < Langue_FR.length; i++) {
                    Langue_FR[i].style.display = 'block';
                }
                for (i = 0; i < Langue_AN.length; i++) {
                    Langue_AN[i].style.display = 'none';
                }
                for (i = 0; i < Langue_AR.length; i++) {
                    Langue_AR[i].style.display = 'none';
                }

                Tout.innerHTML = "Tout";
                Nos_Menus.innerHTML = "Nos Menus";
                Nos_Sandwichs.innerHTML = "Nos Sandwichs";
                Petites_faims.innerHTML = "Petites faims";
                Nos_salades.innerHTML = "Nos salades";
                Nos_desserts.innerHTML = "Nos desserts";
                Nos_boissons.innerHTML = "Nos boissons";
                Nos_frites.innerHTML = "Nos frites";
                Nos_sauces.innerHTML = "Nos sauces";
                McCafe.innerHTML = "Nos Café";
                
            }else if(sessionStorage.getItem("langue") == "AN"){
                //ANGLISH
                for (i = 0; i < Langue_FR.length; i++) {
                    Langue_FR[i].style.display = 'none';
                }
                for (i = 0; i < Langue_AN.length; i++) {
                    Langue_AN[i].style.display = 'block';
                }
                for (i = 0; i < Langue_AR.length; i++) {
                    Langue_AR[i].style.display = 'none';
                }

                Tout.innerHTML = "All";
                Nos_Menus.innerHTML = "Our menus";
                Nos_Sandwichs.innerHTML = "Our Sandwiches";
                Petites_faims.innerHTML = "Small appetites";
                Nos_salades.innerHTML = "Our salads";
                Nos_desserts.innerHTML = "Our desserts";
                Nos_boissons.innerHTML = "Our drinks";
                Nos_frites.innerHTML = "Our fries";
                Nos_sauces.innerHTML = "Our sauces";
                McCafe.innerHTML = "Our Coffee";
            }else if(sessionStorage.getItem("langue") == "AR"){
                //ARABE
                for (i = 0; i < Langue_FR.length; i++) {
                    Langue_FR[i].style.display = 'none';
                }
                for (i = 0; i < Langue_AN.length; i++) {
                    Langue_AN[i].style.display = 'none';
                }
                for (i = 0; i < Langue_AR.length; i++) {
                    Langue_AR[i].style.display = 'block';
                }
                Tout.innerHTML = "الكل";
                Nos_Menus.innerHTML = "قوائمنا";
                Nos_Sandwichs.innerHTML = "شطائرنا";
                Petites_faims.innerHTML = "القليل من الجوع";
                Nos_salades.innerHTML = "سلطاتنا";
                Nos_desserts.innerHTML = "حلوياتنا";
                Nos_boissons.innerHTML = "مشروباتنا";
                Nos_frites.innerHTML = "البطاطس المقلية";
                Nos_sauces.innerHTML = "صلصاتنا";
                McCafe.innerHTML = "قهوتنا";
            }
        } else {
            alert("Sorry, your browser does not support Web Storage...");
        }
    }

    function FR(){
        for (i = 0; i < Langue_FR.length; i++) {
            Langue_FR[i].style.display = 'block';
        }
       for (i = 0; i < Langue_AN.length; i++) {
            Langue_AN[i].style.display = 'none';
        }
       for (i = 0; i < Langue_AR.length; i++) {
