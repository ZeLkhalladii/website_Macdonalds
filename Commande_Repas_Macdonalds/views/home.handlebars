    function AR(){
        for (i = 0; i < Langue_FR.length; i++) {
            Langue_FR[i].style.display = 'none';
        }
       for (i = 0; i < Langue_AN.length; i++) {
            Langue_AN[i].style.display = 'none';
        }
       for (i = 0; i < Langue_AR.length; i++) {
            Langue_AR[i].style.display = 'block';
        }
        Tout.innerHTML = "الكل";
        Nos_Menus.innerHTML = "قوائمنا";
